
<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <title>OoT Bingo</title>
    <script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="/css/board.css">
</head>
<body>
    <table class="bingo bingo-element <%= hidden ? 'hidden' : '' %>">
    <% for (var i = 0; i < 5; i++) { %>
    <tr>
      <% for (var j = 0; j < 5; j++) { %>
        <% var x = i * 5 + j %>
      <td id="square<%= x %>" class="square row<%= i %> col<%= j %> <%= i === j ? 'tlbr' : '' %> <%= i === 5 - j ? 'bltr' : '' %> <%= colours[x].join(' ') %>"><%= board[x] ? board[x] : '' %></td>
      <% } %>
    </tr>
    <% } %>
</table>

<script src="/js/sync.js"></script>

</body>
</html>
