<html>
<head>
  <script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>
  <script src="/js/timer.js"></script>
  <link rel="stylesheet" type="text/css" href="/css/timer.css">
</head>
<body>
  <%
  var placeClass = ''
  switch(place) {
    case('1st')
      placeClass = 'gold'
      break;
    case('2nd')
      placeClass = 'silver'
      break;
    case('3rd')
      placeClass = 'bronze'
      break;
    case('')
      placeClass = 'hidden'
      break;
  }
  %>
  <div class="place <%= placeClass %>"><%= place %></div>
  <div class="timer" data-starttime="<%= startTime %>" data-finaltime="<%= finalTime %>">
    <span class="hours"></span><span class="separator">:</span><span class="minutes">0</span><span class="separator">:</span><span class="seconds">00</span>
  </div>
</body>
